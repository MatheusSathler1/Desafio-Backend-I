{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "object-curly-spacing": 0,
        "new-cap": 0,
        "space-before-function-paren": 0,
        "indent": ["error", 4]
    }
}
